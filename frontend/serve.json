{
  "rewrites": [
    { "source": "!/api/**", "destination": "/index.html" }
  ],
  "headers": [
    {
      "source" : "**/*.@(jpg|jpeg|gif|png|svg)",
      "headers" : [{
        "key" : "Cache-Control",
        "value" : "public, max-age=31536000, immutable"
      }]
    }
  ],
  "proxy": [
    {
      "source": "/api",
      "destination": "http://backend:8000"
    }
  ]
}
